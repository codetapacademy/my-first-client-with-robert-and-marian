.menu
  if showBackButton
    a(href=showBackButton).menu__disc
      .icon__circle-left
  .menu__title
  //- h2.menu__title Profesional Mastic Applier
  input(type="checkbox" id="menu")
  label(for="menu").menu__disc
    .icon__menu7
    .icon__cross
  .menu__list
    each item in data.menu
      if item.label == landingPage
        a.highlight(
          href=`${baseUrl}${item.link}`
          title=item.label
        )=item.label
      else
        a(
          href=`${baseUrl}${item.link}`
          title=item.label
        )=item.label

